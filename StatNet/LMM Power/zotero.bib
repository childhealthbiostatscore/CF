@article{kohler1264PTrendsIncidence2023,
  title = {1264-{{P}}: {{Trends}} in {{Incidence}} and {{Prevalence}} of {{CFRD}} from the {{U}}.{{S}}. {{Cystic Fibrosis Foundation Patient Registry}} ({{CFFPR}})},
  shorttitle = {1264-{{P}}},
  author = {Kohler, Celia L. and Vigers, Timothy B. and Sakamoto, Casey and Moran, Antoinette and Sagel, Scott and Chan, Christine L.},
  year = {2023},
  month = jun,
  journal = {Diabetes},
  volume = {72},
