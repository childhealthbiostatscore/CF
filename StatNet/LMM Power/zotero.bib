@article{shiUnsupervisedDiscoveryPhenotypespecific2019,
  title = {Unsupervised Discovery of Phenotype-Specific Multi-Omics Networks},
  author = {Shi, W Jenny and Zhuang, Yonghua and Russell, Pamela H and Hobbs, Brian D and Parker, Margaret M and Castaldi, Peter J and Rudra, Pratyaydipta and Vestal, Brian and Hersh, Craig P and Saba, Laura M and Kechris, Katerina},
  editor = {Birol, Inanc},
  year = {2019},
